import wollok.game.*
import config.*
import jugador.*
import item.*
import nivel.*

program main {
	game.height(config.alto())
	game.width(config.ancho())
	game.cellSize(config.celdas())
	game.title("Moving Out")
	game.boardGround("fondo.png")
	game.addVisual(jugador)
	game.showAttributes(jugador)
	
	config.teclasJugador()
	nivel.ejecutar()
	
	game.start()
}
